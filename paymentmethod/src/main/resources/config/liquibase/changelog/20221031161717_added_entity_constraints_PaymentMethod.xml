<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity PaymentMethod.
    -->
    <changeSet id="20221031161717-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="related_place_id"
                                 baseTableName="payment_method"
                                 constraintName="fk_payment_method__related_place_id"
                                 referencedColumnNames="id"
                                 referencedTableName="related_place"/>

        <addForeignKeyConstraint baseColumnNames="related_party_id"
                                 baseTableName="payment_method"
                                 constraintName="fk_payment_method__related_party_id"
                                 referencedColumnNames="id"
                                 referencedTableName="related_party"/>
    </changeSet>
</databaseChangeLog>
